{
    "name": "clainy/openxpan-apiclient-php",
    "description": "百度网盘开发平台API",
    "homepage": "https://www.github.com/clainy/openxpan-apiclient-php",
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Clainy\\BaiduXpanApi\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Clainy\\BaiduXpanApi\\Test\\" : "test/" }
    }
}
